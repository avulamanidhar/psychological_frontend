            app:destination="@id/moodSelectionFragment" />
        <action
            android:id="@+id/action_chatFragment_to_toolsFragment"
            app:destination="@id/toolsFragment" />
    </fragment>

    <fragment
        android:id="@+id/toolsFragment"
        android:name="com.example.mindguardaipsychologicalsupportapp.ToolsFragment"
