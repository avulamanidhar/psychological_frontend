        tools:layout="@layout/fragment_language_accessibility">
        <action
            android:id="@+id/action_languageAccessibilityFragment_to_stayConnectedFragment"
            app:destination="@id/stayConnectedFragment" />
    </fragment>

    <fragment
        android:id="@+id/stayConnectedFragment"
        android:name="com.example.mindguardaipsychologicalsupportapp.StayConnectedFragment"
        android:label="StayConnectedFragment"
        tools:layout="@layout/fragment_stay_connected">
        <action
            android:id="@+id/action_stayConnectedFragment_to_profileOverviewFragment"
            app:destination="@id/profileOverviewFragment" />
    </fragment>

    <fragment
        android:id="@+id/profileOverviewFragment"
        android:name="com.example.mindguardaipsychologicalsupportapp.ProfileOverviewFragment"
        android:label="ProfileOverviewFragment"
        tools:layout="@layout/fragment_profile_overview">
        <action
            android:id="@+id/action_profileOverviewFragment_to_homeFragment"
            app:destination="@id/homeFragment" />
    </fragment>

    <fragment
        android:id="@+id/homeFragment"
        android:name="com.example.mindguardaipsychologicalsupportapp.HomeFragment"
        android:label="HomeFragment"
        tools:layout="@layout/fragment_home">
