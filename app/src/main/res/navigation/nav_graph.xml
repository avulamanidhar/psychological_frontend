    <fragment
        android:id="@+id/detectedPatternsFragment"
        android:name="com.example.mindguardaipsychologicalsupportapp.DetectedPatternsFragment"
        android:label="DetectedPatternsFragment"
        tools:layout="@layout/fragment_detected_patterns" />

    <fragment
        android:id="@+id/todayVsLastWeekFragment"
        android:name="com.example.mindguardaipsychologicalsupportapp.TodayVsLastWeekFragment"
        android:label="TodayVsLastWeekFragment"
        tools:layout="@layout/fragment_today_vs_last_week" />

    <fragment
        android:id="@+id/personalizedRecommendationsFragment"
        android:name="com.example.mindguardaipsychologicalsupportapp.PersonalizedRecommendationsFragment"
        android:label="PersonalizedRecommendationsFragment"
        tools:layout="@layout/fragment_personalized_recommendations" />

    <fragment
        android:id="@+id/aiAnalysisFragment"
        android:name="com.example.mindguardaipsychologicalsupportapp.AIAnalysisFragment"
        android:label="AIAnalysisFragment"
        tools:layout="@layout/fragment_ai_analysis" />

</navigation>