    <fragment
        android:id="@+id/aiInsightsFragment"
        android:name="com.example.mindguardaipsychologicalsupportapp.AIInsightsFragment"
        android:label="AIInsightsFragment"
        tools:layout="@layout/fragment_ai_insights">
        <action
            android:id="@+id/action_aiInsightsFragment_to_moodTrendsFragment"
            app:destination="@id/moodTrendsFragment" />
        <action
            android:id="@+id/action_aiInsightsFragment_to_keyIndicatorsFragment"
            app:destination="@id/keyIndicatorsFragment" />
        <action
            android:id="@+id/action_aiInsightsFragment_to_detectedPatternsFragment"
            app:destination="@id/detectedPatternsFragment" />
        <action
            android:id="@+id/action_aiInsightsFragment_to_todayVsLastWeekFragment"
            app:destination="@id/todayVsLastWeekFragment" />
        <action
            android:id="@+id/action_aiInsightsFragment_to_personalizedRecommendationsFragment"
            app:destination="@id/personalizedRecommendationsFragment" />
        <action
            android:id="@+id/action_aiInsightsFragment_to_aiAnalysisFragment"
            app:destination="@id/aiAnalysisFragment" />
    </fragment>

