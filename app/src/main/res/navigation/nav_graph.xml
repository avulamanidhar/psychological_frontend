            app:destination="@id/toolsFragment" />
        <action
            android:id="@+id/action_moodSelectionFragment_to_moodIntensityFragment"
            app:destination="@id/moodIntensityFragment" />
    </fragment>

    <fragment
        android:id="@+id/moodIntensityFragment"
        android:name="com.example.mindguardaipsychologicalsupportapp.MoodIntensityFragment"
